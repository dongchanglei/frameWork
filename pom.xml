<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">

    <modelVersion>4.0.0</modelVersion>

    <groupId>com.dcl</groupId>
    <artifactId>framework</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <name>framework</name>
    <description>frameWork project for Spring Boot</description>

    <packaging>pom</packaging>

    <modules>
        <module>custom-spring</module>
    </modules>

    <!-- 由于parent的存在 spring-boot 统一管理了相关依赖和版本 子模块自动会继承 -->
    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.5.4</version>
        <relativePath/> <!-- lookup parent from repository -->
    </parent>

    <properties>
        <java.version>1.8</java.version>
    </properties>

    <!-- 依赖管理 这里面就不需要spring-boot相关配置因为已配置了parent  dependencyManagement管理的依赖需要版本号 -->
    <dependencyManagement>
        <dependencies>

        </dependencies>
    </dependencyManagement>

</project>
